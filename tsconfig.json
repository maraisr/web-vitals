{
    "extends": "@marais/tsconfig",
    "compilerOptions": {
      "strict": true,
      "target": "es2019",
      "module": "esnext",
      "strictFunctionTypes": true,
      "strictNullChecks": true,
      "noImplicitAny": true,
      "allowJs": false,
      "lib": ["ES2020", "WebWorker"],
      "types": ["@cloudflare/workers-types"]
    },
    "include": [
      "src",
    ],
    "exclude": [
      "node_modules"
    ]
  }